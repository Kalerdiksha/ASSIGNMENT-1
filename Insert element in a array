#include<iostream>
using namespace std;
int main()
{
    int arr[7], i, elem;
    cout<<"Enter 6 Array Elements: ";
    for(i=0; i<6; i++)
        cin>>arr[i];
    cout<<"\nEnter Element to Insert: ";
    cin>>elem;
    arr[i] = elem;
    cout<<"\nThe New Array is:\n";
    for(i=0; i<7; i++)
        cout<<arr[i]<<"  ";
    cout<<endl;
    return 0;
}

